{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "webapp"],
  "include": ["source/*.ts", ".eleventy.ts"]
}
